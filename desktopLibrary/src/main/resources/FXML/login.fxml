<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.controllers.LoginController">
   <children>
      <TextField fx:id="username" layoutX="152.0" layoutY="127.0" />
      <Button layoutX="152.0" layoutY="233.0" mnemonicParsing="false" onAction="#loginButtonClicked" prefHeight="37.0" prefWidth="84.0" text="Login">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Label layoutX="70.0" layoutY="130.0" text="Username:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="74.0" layoutY="180.0" text="Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="246.0" layoutY="233.0" mnemonicParsing="false" onAction="#backToEntryClicked" text="Back">
         <font>
            <Font name="System Italic" size="16.0" />
         </font>
      </Button>
      <Label fx:id="errorLabel" layoutX="8.0" layoutY="44.0" prefHeight="20.0" prefWidth="382.0" textAlignment="CENTER" textFill="#ee0202">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="password" layoutX="152.0" layoutY="178.0" />
      <Text layoutX="172.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
