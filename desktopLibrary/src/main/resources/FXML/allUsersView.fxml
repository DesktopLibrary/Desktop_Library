<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.controllers.ViewAllUsersAdminController">
    <children>
        <Button fx:id="moreInfoButton" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#moreInfoClicked" prefHeight="30.0" prefWidth="96.0" text="More Info">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button fx:id="deleteButton" layoutX="120.0" layoutY="361.0" mnemonicParsing="false" onAction="#deleteButtonClicked" prefHeight="25.0" prefWidth="67.0" text="Delete">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <TableView fx:id="table" layoutY="33.0" prefHeight="313.0" prefWidth="401.0">
            <columns>
                <TableColumn prefWidth="193.0" text="Username">
                    <cellValueFactory>
                        <PropertyValueFactory property="username" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="193.0" text="Email">
                    <cellValueFactory>
                        <PropertyValueFactory property="email" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Label fx:id="errorLabel" alignment="CENTER" layoutX="7.0" layoutY="4.0" prefHeight="20.0" prefWidth="387.0" textFill="#fc0202" AnchorPane.topAnchor="8.0">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Button fx:id="backButton" layoutX="338.0" layoutY="361.0" mnemonicParsing="false" onAction="#backToMainMenuClicked" prefHeight="25.0" prefWidth="49.0" text="Back">
            <font>
                <Font name="System Italic" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
