<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.controllers.MenuController">
   <children>
      <Label fx:id="loggedName" alignment="CENTER" layoutX="95.0" layoutY="70.0" prefHeight="20.0" prefWidth="211.0">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button layoutX="125.0" layoutY="128.0" mnemonicParsing="false" onAction="#viewBooksClicked" text="My Books" AnchorPane.leftAnchor="125.0" AnchorPane.rightAnchor="125.0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="125.0" layoutY="240.0" mnemonicParsing="false" onAction="#editProfileClicked" text="Edit Profile" AnchorPane.leftAnchor="125.0" AnchorPane.rightAnchor="125.0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="126.0" layoutY="185.0" mnemonicParsing="false" onAction="#addBookClicked" text="New Book" AnchorPane.leftAnchor="126.0" AnchorPane.rightAnchor="124.0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="312.0" layoutY="356.0" mnemonicParsing="false" onAction="#exitClicked" prefHeight="30.0" prefWidth="74.0" text="Log Out" AnchorPane.leftAnchor="312.0" AnchorPane.rightAnchor="14.0">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
